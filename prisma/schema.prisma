generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Client {
  userId      String @unique
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  email       String @unique
  clientName  String
  age         Int
  height      Float
  weight      Float
  phoneNumber Int
}
